section.login
	form.login-form
		legend.login__title Contact form
		label Email
			input(type='email' placeholder='Email')
		label Password
			input(type='password' placeholder='Password')
		label Age
			input(type='number' placeholder='Type number')
		
		label Remember me
			input(type='checkbox')
			span.login__indicator
		
		
		button.login__sign-in(type='submit') Sign in
		
	
	
